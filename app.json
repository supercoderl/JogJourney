{
  "expo": {
    "name": "JogJourney",
    "slug": "jog-journey",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/background.png"
      },
      "package": "com.k16.jogjourney",
      "versionCode": 1
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/background.png",
          "imageWidth": 200,
          "resizeMode": "contain"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Inter-Black.otf",
            "./assets/fonts/Inter-BlackItalic.otf",
            "./assets/fonts/Inter-Bold.otf",
            "./assets/fonts/Inter-BoldItalic.otf",
            "./assets/fonts/Inter-ExtraBold.otf",
            "./assets/fonts/Inter-ExtraBoldItalic.otf",
            "./assets/fonts/Inter-ExtraLight.otf",
            "./assets/fonts/Inter-ExtraLightItalic.otf",
            "./assets/fonts/Inter-Italic.otf",
            "./assets/fonts/Inter-Light.otf",
            "./assets/fonts/Inter-LightItalic.otf",
            "./assets/fonts/Inter-Medium.otf",
            "./assets/fonts/Inter-MediumItalic.otf",
            "./assets/fonts/Inter-Regular.otf",
            "./assets/fonts/Inter-SemiBold.otf",
            "./assets/fonts/Inter-SemiBoldItalic.otf",
            "./assets/fonts/Inter-Thin.otf",
            "./assets/fonts/Inter-ThinItalic.otf",
            "./assets/fonts/Inter-V.ttf",
            "./assets/fonts/Jomhuria-Regular.ttf",
            "./assets/fonts/SpaceMono-Regular.ttf",
            "./assets/fonts/Lohit-Bengali.ttf"
          ]
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Cho phép $(PRODUCT_NAME) sử dụng vị trí của bạn."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Ứng dụng cần truy cập vào thư viện ảnh của bạn để chọn hình ảnh."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "aa8ab995-d704-4d87-a917-02ca787770d5"
      },
      "router": {
        "origin": false
      }
    },
    "owner": "pronative"
  }
}